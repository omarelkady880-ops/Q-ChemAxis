# Copy this file to .env and set your actual values

# Mistral AI API Key (get from https://console.mistral.ai/)
MISTRAL_API_KEY=your-mistral-api-key-here

# Session secret for secure session management
SESSION_SECRET=your-strong-random-session-secret-change-in-production

# JWT secret (optional, for future JWT implementation)
JWT_SECRET=your-strong-random-jwt-secret-change-in-production

# Server port (default: 3001)
PORT=3001

# Node environment (development/production)
NODE_ENV=development
